{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Author": {
      "type": "string",
      "required": true
    },
    "Pages": {
      "type": "integer",
      "required": true
    },
    "Rating": {
      "type": "integer",
      "required": true,
      "max": 5,
      "min": 1,
      "default": 0
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "books"
    }
  }
}
